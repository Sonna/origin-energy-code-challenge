{
  "name": "origin-energy-code-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b && vite build && npm run build:server",
    "build:client": "tsc -p tsconfig.client.json",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "vite",
    "format": "npx prettier --cache --write src && eslint --cache --cache-location 'node_modules/.cache/eslint/.eslintcache' --fix src",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:tsc",
    "lint:eslint": "eslint --cache --cache-location 'node_modules/.cache/eslint/.eslintcache' src",
    "lint:prettier": "prettier --cache --list-different src",
    "lint:tsc": "tsc",
    "preview": "vite preview",
    "start": "node ./dist/server.js",
    "test": "jest",
    "test:cypress-gui": "npm --prefix ./integration run test:cypress-gui",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.29",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@jest/globals": "^30.0.0-beta.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "dedent": "^1.6.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  }
}
